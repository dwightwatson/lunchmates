<h2>All lunches</h2>

<table class="table">
  <thead>
    <tr>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <%= for lunch <- @lunches do %>
      <tr>
        <td>
          <%= link lunch.date, to: lunch_path(@conn, :show, lunch) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
